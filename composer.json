{
    "name": "sicope-model/sicope-model",
    "description": "Model Based Testing Tool",
    "homepage": "https://github.com/sicope-model/sicope-model",
    "license": "MIT",
    "type": "project",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "composer/package-versions-deprecated": "^1.8",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/orm": "^2.6",
        "easycorp/easyadmin-bundle": "^3.5",
        "erusev/parsedown": "^1.6",
        "sensio/framework-extra-bundle": "^6.1",
        "stakovicz/ux-collection": "^0.0.4",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "^5.3",
        "symfony/console": "^5.3",
        "symfony/dotenv": "^5.3",
        "symfony/expression-language": "^5.3",
        "symfony/flex": "^1.1",
        "symfony/form": "^5.3",
        "symfony/framework-bundle": "^5.3",
        "symfony/intl": "^5.3",
        "symfony/mailer": "^5.3",
        "symfony/monolog-bundle": "^3.1",
        "symfony/notifier": "5.3.*",
        "symfony/polyfill-intl-messageformatter": "^1.12",
        "symfony/runtime": "^5.3",
        "symfony/security-bundle": "^5.3",
        "symfony/slack-notifier": "5.3.*",
        "symfony/string": "^5.3",
        "symfony/translation": "^5.3",
        "symfony/twig-pack": "^1.0",
        "symfony/validator": "^5.3",
        "symfony/webpack-encore-bundle": "^1.4",
        "symfony/yaml": "^5.3",
        "tgalopin/html-sanitizer-bundle": "^1.2",
        "tienvx/mbt-bundle": "^3.0@alpha",
        "twig/intl-extra": "^3.0",
        "twig/markdown-extra": "^3.0"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.2",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "require": "5.3.*",
            "allow-contrib": true
        }
    }
}
