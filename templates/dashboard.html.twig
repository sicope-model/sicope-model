{# @var ea \EasyCorp\Bundle\EasyAdminBundle\Context\AdminContext #}
{% extends '@EasyAdmin/page/content.html.twig' %}

{% block head_stylesheets %}
    {{ encore_entry_link_tags('app') }}
    {{ parent() }}
{% endblock %}

{% block head_javascript %}
    {{ encore_entry_script_tags('app') }}
    {{ parent() }}
{% endblock %}

{% block main %}
    <main>

        <div class="content">
            <div class="row">
                <div class="col-md-12 page-header">
                    <h2 class="page-title">Dashboard</h2>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-6 col-md-6 col-lg-3">
                    <div class="card">
                        <div class="content">
                            <div class="row">
                                <div class="dfd text-center">
                                    <i class="blue large-icon mb-2 fas fa-bug"></i>
                                    <h4 class="mb-0">{{ bugs }}</h4>
                                    <p class="text-muted">TOTAL BUGS</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-sm-6 col-md-6 col-lg-3">
                    <div class="card">
                        <div class="content">
                            <div class="row">
                                <div class="dfd text-center">
                                    <i class="orange large-icon mb-2 fas fa-tasks"></i>
                                    <h4 class="mb-0">{{ tasks }}</h4>
                                    <p class="text-muted">TOTAL TASKS</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-sm-6 col-md-6 col-lg-3">
                    <div class="card">
                        <div class="content">
                            <div class="row">
                                <div class="dfd text-center">
                                    <i class="grey large-icon mb-2 fas fa-project-diagram"></i>
                                    <h4 class="mb-0">{{ models }}</h4>
                                    <p class="text-muted">TOTAL MODELS</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-sm-6 col-md-6 col-lg-3">
                    <div class="card">
                        <div class="content">
                            <div class="row">
                                <div class="dfd text-center">
                                    <i class="olive large-icon mb-2 fas fa-users"></i>
                                    <h4 class="mb-0">{{ users }}</h4>
                                    <p class="text-muted">TOTAL USERS</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <div class="row">
                        <div class="col-md-6">
                            <div class="card">
                                <div class="content">
                                    <div class="head">
                                        <h5 class="mb-0">Tasks & Bugs Overview</h5>
                                        <p class="text-muted">Tasks and bugs created in the last 12 days</p>
                                    </div>
                                    <div class="canvas-wrapper">
                                        {{ render_chart(taskBugChart) }}
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="card">
                                <div class="content">
                                    <div class="head">
                                        <h5 class="mb-0">Models Overview</h5>
                                        <p class="text-muted">Models created in the last 12 days</p>
                                    </div>
                                    <div class="canvas-wrapper">
                                        {{ render_chart(modelChart) }}
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="card">
                        <div class="content">
                            <div class="head">
                                <h5 class="mb-0">Top Visitors by Country</h5>
                                <p class="text-muted">Current year website visitor data</p>
                            </div>
                            <div class="canvas-wrapper">
                                <table class="table table-striped">
                                    <thead class="success">
                                    <tr>
                                        <th>Country</th>
                                        <th class="text-end">Unique Visitors</th>
                                    </tr>
                                    </thead>
                                    <tbody>
                                    <tr>
                                        <td><i class="flag-icon flag-icon-us"></i> United States</td>
                                        <td class="text-end">27,340</td>
                                    </tr>
                                    <tr>
                                        <td><i class="flag-icon flag-icon-in"></i> India</td>
                                        <td class="text-end">21,280</td>
                                    </tr>
                                    <tr>
                                        <td><i class="flag-icon flag-icon-jp"></i> Japan</td>
                                        <td class="text-end">18,210</td>
                                    </tr>
                                    <tr>
                                        <td><i class="flag-icon flag-icon-gb"></i> United Kingdom</td>
                                        <td class="text-end">15,176</td>
                                    </tr>
                                    <tr>
                                        <td><i class="flag-icon flag-icon-es"></i> Spain</td>
                                        <td class="text-end">14,276</td>
                                    </tr>
                                    <tr>
                                        <td><i class="flag-icon flag-icon-de"></i> Germany</td>
                                        <td class="text-end">13,176</td>
                                    </tr>
                                    <tr>
                                        <td><i class="flag-icon flag-icon-br"></i> Brazil</td>
                                        <td class="text-end">12,176</td>
                                    </tr>
                                    <tr>
                                        <td><i class="flag-icon flag-icon-id"></i> Indonesia</td>
                                        <td class="text-end">11,886</td>
                                    </tr>
                                    <tr>
                                        <td><i class="flag-icon flag-icon-ph"></i> Philippines</td>
                                        <td class="text-end">11,509</td>
                                    </tr>
                                    <tr>
                                        <td><i class="flag-icon flag-icon-nz"></i> New Zealand</td>
                                        <td class="text-end">1,700</td>
                                    </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="card">
                        <div class="content">
                            <div class="head">
                                <h5 class="mb-0">Most Visited Pages</h5>
                                <p class="text-muted">Current year website visitor data</p>
                            </div>
                            <div class="canvas-wrapper">
                                <table class="table table-striped">
                                    <thead class="success">
                                    <tr>
                                        <th>Page Name</th>
                                        <th class="text-end">Visitors</th>
                                    </tr>
                                    </thead>
                                    <tbody>
                                    <tr>
                                        <td>/about.html <a href="#"><i class="fas fa-link blue"></i></a></td>
                                        <td class="text-end">8,340</td>
                                    </tr>
                                    <tr>
                                        <td>/special-promo.html <a href="#"><i class="fas fa-link blue"></i></a></td>
                                        <td class="text-end">7,280</td>
                                    </tr>
                                    <tr>
                                        <td>/products.html <a href="#"><i class="fas fa-link blue"></i></a></td>
                                        <td class="text-end">6,210</td>
                                    </tr>
                                    <tr>
                                        <td>/documentation.html <a href="#"><i class="fas fa-link blue"></i></a></td>
                                        <td class="text-end">5,176</td>
                                    </tr>
                                    <tr>
                                        <td>/customer-support.html <a href="#"><i class="fas fa-link blue"></i></a></td>
                                        <td class="text-end">4,276</td>
